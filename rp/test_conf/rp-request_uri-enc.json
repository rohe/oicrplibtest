{
  "issuer": "<OP>/oicrp/rp-request_uri-enc",
  "redirect_uris": [
    "<RP>/ihf_cb/rp-request_uri-enc"
  ],
  "client_preferences": {
    "application_type": "web",
    "application_name": "rphandler",
    "contacts": [
      "ops@example.com"
    ],
    "response_types": [
      "<RESPONSE_TYPE>"
    ],
    "scope": [
      "openid"
    ],
    "token_endpoint_auth_method": "client_secret_basic"
  },
  "jwks_uri": "<RP>/static/jwks.json",
  "requests_dir": "static",
  "services": {
    "discovery": {
      "class": "oidcservice.oidc.provider_info_discovery.ProviderInfoDiscovery"
    },
    "registration": {
      "class": "oidcservice.oidc.registration.Registration"
    },
    "authorization": {
      "class": "oidcservice.oidc.authorization.Authorization",
      "kwargs": {
        "pre_construct": {
          "request_method": "request_uri"
        },
        "post_construct": {
          "request_method": "request_uri",
          "request_object_signing_alg": "none",
          "request_object_encryption_alg": "RSA1_5",
          "request_object_encryption_enc": "A128CBC-HS256",
          "target": "https://localhost:8080/oicrp/rp-request_uri-enc"
        }
      }
    },
    "access_token": {
      "class": "oidcservice.oidc.access_token.AccessToken"
    }
  }
}